<?xml version="1.0" encoding="UTF-8"?>
<opml version="2.0">
  <body>
    <outline text="Naren's Podcasts" title="Naren's Podcasts">
      {% assign directory = site.data.directory %}
      {% for section in directory.sections %}
      {% for site in section.sites %}
      {% if site.rss_feed != blank %}
      {% if site.kind == 'podcast' %}
      <outline text="{{ site.name }}" title="{{ site.name }}" xmlUrl="{{ site.rss_feed }}" type="rss" />
      {% endif %}
      {% endif %}
      {% endfor %}
      {% endfor %}
    </outline>
  </body>
</opml>
